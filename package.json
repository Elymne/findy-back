{
    "name": "findy-back",
    "version": "0.0.0",
    "private": true,
    "main": "dist/src/index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon --watch src/ -e ts --exec \"ts-node src/index.ts\"",
        "lint": "eslint . --ext .ts",
        "prebuild": "rimraf dist",
        "build": "tsc && tsc-alias && rimraf dist/__tests__",
        "test": "jest __tests_ --coverage=true --coverageDirectory=coverage",
        "start": "cross-env NODE_ENV=production node dist/src/index.js",
        "postinstall": "npm run build",
        "heroku-postbuild": "mkdir ./.cache && mv /app/.cache/puppeteer ./.cache"
    },
    "dependencies": {
        "apicache": "^1.6.3",
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.2.0",
        "express-validator": "^7.0.1",
        "mongodb": "^6.5.0",
        "puppeteer": "^22.6.5",
        "reflect-metadata": "^0.2.1",
        "rimraf": "^5.0.5",
        "ts-log": "^2.2.5",
        "ts-postgres": "^1.9.0"
    },
    "devDependencies": {
        "@types/apicache": "^1.6.6",
        "@types/body-parser": "^1.19.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.24",
        "@types/validator": "^13.11.9",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "nodemon": "^3.0.3",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    }
}
