        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "express-validator": "^7.2.1",
        "helmet": "^8.1.0",
        "kysely": "^0.27.6",
        "mysql2": "^3.14.0",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^6.0.1",
        "ts-log": "^2.2.7",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
