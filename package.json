        "@types/validator": "^13.12.2",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "eslint": "^9.23.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "ts-jest": "^29.3.0",
        "ts-node": "^10.9.2",
